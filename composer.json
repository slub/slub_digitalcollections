{
  "name": "slub/slub-digitalcollections",
  "type": "typo3-cms-extension",
  "description": "Templates, Styles and Configuration for the Kitodo.Presentation based Digital Collections by SLUB Dresden",
  "homepage": "https://github.com/slub/slub_digitalcollections",
  "keywords": [
    "TYPO3",
    "extension",
    "slub_digitalcollections"
  ],
  "authors": [
    {
      "name": "SLUB TYPO3 Team",
      "email": "typo3@slub-dresden.de",
      "homepage": "https://www.slub-dresden.de",
      "role": "Maintainer"
    },
    {
      "name": "Alexander Bigga",
      "role": "Developer"
    },
    {
      "name": "Christof Rodejohann",
      "role": "Developer"
    },
    {
      "name": "Norman Steger",
      "role": "Developer"
    },
    {
      "name": "Beatrycze Volk",
      "role": "Developer"
    },
    {
      "name": "Thomas Jung",
      "email": "slub@jung.digital",
      "role": "Developer"
    }
  ],
  "license": "GPL-3.0-or-later",
  "support": {
    "email": "typo3@slub-dresden.de",
    "issues": "https://github.com/slub/slub_digitalcollections/issues",
    "source": "https://github.com/slub/slub_digitalcollections"
  },
  "require": {
    "php": "8.2 - 8.4",
    "typo3/cms-core": "^12.4|^13.4",
    "kitodo/presentation": "dev-main"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.12"
  },
  "autoload": {
    "psr-4": {
      "Slub\\SlubDigitalcollections\\": "Classes/"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "slub_digitalcollections"
    }
  },
  "config": {
    "allow-plugins": {
      "typo3/class-alias-loader": true,
      "typo3/cms-composer-installers": true
    }
  },
  "repositories": [
    {
      "type": "github",
      "url": "https://github.com/kitodo/php-iiif-prezi-reader.git"
    }
  ]
}
